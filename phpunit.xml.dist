<?xml version="1.0"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/10.4/phpunit.xsd" bootstrap="tests/bootstrap.php">
  <testsuites>
    <testsuite name="Tests Suite">
      <directory>tests</directory>
    </testsuite>
  </testsuites>
  <coverage pathCoverage="true"/>
  <source>
    <include>
      <directory suffix=".php">src</directory>
    </include>
  </source>
</phpunit>
